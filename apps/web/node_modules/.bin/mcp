#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/sdk/bin/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/sdk/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/sdk/bin/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/sdk/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules/@vercel/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/@vercel+sdk@1.17.0/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/sdk/bin/mcp-server.js" "$@"
else
  exec node  "$basedir/../@vercel/sdk/bin/mcp-server.js" "$@"
fi
