{
  "name": "coding-agent-template",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "turbo run test",
    "test:credentials": "tsx --env-file=.env.local scripts/validate-credentials.ts",
    "analyze": "turbo run analyze",
    "analyze:web": "pnpm --filter @repo/web analyze",
    "analyze:playground": "pnpm --filter @repo/playground-vite analyze",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:rollback": "drizzle-kit migrate --to 0",
    "db:seeds": "tsx --env-file=.env.local scripts/seeds.ts",
    "db:fixtures": "tsx --env-file=.env.local scripts/fixtures.ts",
    "docker:setup": "bash scripts/wsl-docker-setup.sh",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:status": "docker-compose ps",
    "docker:clean": "docker-compose down -v",
    "prepare": "husky",
    "bootstrap": "bash scripts/ds-bootstrap.sh"
  },
  "dependencies": {
    "@git-diff-view/file": "^0.0.32",
    "@git-diff-view/react": "^0.0.32",
    "@monaco-editor/react": "^4.7.0",
    "@neondatabase/serverless": "^0.10.1",
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/js-cookie": "^3.0.6",
    "@types/ws": "^8.18.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/sandbox": "^0.0.21",
    "@vercel/sdk": "^1.13.9",
    "ai": "5.0.51",
    "arctic": "^3.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.0",
    "drizzle-orm": "^0.36.4",
    "jose": "^6.1.0",
    "jotai": "^2.15.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "ms": "^2.1.3",
    "nanoid": "^5.1.5",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.4.0",
    "tailwind-merge": "^3.3.1",
    "typescript-language-server": "^5.0.1",
    "vaul": "^1.1.2",
    "vscode-jsonrpc": "^8.2.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/ms": "^2.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-promise": "^7.2.1",
    "@next/bundle-analyzer": "^15.1.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "husky": "^9.1.7",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "turbo": "^2.3.3",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
