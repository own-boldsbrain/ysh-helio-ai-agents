#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rookie/projects/coding-agent-template/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@3.2.4_jiti@2.6.1_jsdom@26.1._emzvwl7iqyaioqvf2yiv46j7lq/node_modules/vitest/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@3.2.4_jiti@2.6.1_jsdom@26.1._emzvwl7iqyaioqvf2yiv46j7lq/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rookie/projects/coding-agent-template/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@3.2.4_jiti@2.6.1_jsdom@26.1._emzvwl7iqyaioqvf2yiv46j7lq/node_modules/vitest/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@3.2.4_jiti@2.6.1_jsdom@26.1._emzvwl7iqyaioqvf2yiv46j7lq/node_modules:/home/rookie/projects/coding-agent-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
