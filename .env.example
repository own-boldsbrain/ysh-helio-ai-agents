# ============================================
# DATABASE CONFIGURATION (Required)
# ============================================
POSTGRES_URL=postgresql://postgres:password@localhost:5433/coding_agent

# ============================================
# NEXTAUTH CONFIGURATION (Required)
# ============================================
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="changeme_32b"

# ============================================
# ENCRYPTION & SECURITY (Required)
# ============================================
JWE_SECRET=changeme_32b_base64_encoded_secret_for_jwe
ENCRYPTION_KEY=0123456789abcdef0123456789abcdef

# ============================================
# AUTHENTICATION PROVIDERS (Required)
# ============================================
NEXT_PUBLIC_AUTH_PROVIDERS="github"

# GitHub OAuth (Required if using GitHub auth)
NEXT_PUBLIC_GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# GitHub App Configuration (Optional)
GITHUB_APP_ID=your_github_app_id_here
GITHUB_PRIVATE_KEY_PATH=/path/to/your/github-private-key.pem

# ============================================
# SANDBOX CONFIGURATION (Required)
# ============================================
# Options: 'vercel' or 'docker'
SANDBOX_PROVIDER=docker

# Docker Sandbox Configuration (Required if SANDBOX_PROVIDER=docker)
# Base image (Alpine-based, lightweight)
SANDBOX_DOCKER_IMAGE=coding-agent-sandbox:latest
# Specialized images for specific languages:
# - coding-agent-sandbox:python (Python + scientific libraries)
# - coding-agent-sandbox:java (Java 21 + Maven + Gradle)
# - coding-agent-sandbox:nodejs (Node.js 20 + modern tooling)

DOCKER_NETWORK=coding-agent-network

# Docker Resource Limits (Optional)
SANDBOX_MEMORY_LIMIT=2g
SANDBOX_CPU_LIMIT=2

# Volume Management (Optional)
# Keep volumes after sandbox stops (useful for debugging)
SANDBOX_KEEP_VOLUME=false

# Docker Hub Credentials (Optional - for private images)
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_token_or_password

# ============================================
# AI API KEYS (Optional but recommended)
# ============================================
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Groq
GROQ_API_KEY=your_groq_api_key_here

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_GEMINI_API_KEY_2=your_second_gemini_key_here

# NVIDIA
NVIDIA_API_KEY=your_nvidia_api_key_here

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Ollama Configuration (Optional)
OLLAMA_HOST=http://host.docker.internal:11434

# ============================================
# AI MODEL CONFIGURATION (Optional)
# ============================================
QWEN_PROVIDER="openrouter"
QWEN_MODEL_PLANNER="qwen2.5-72b-instruct"
QWEN_MODEL_CODER="qwen2.5-coder-32b-instruct"
QWEN_MODEL_DOCS="qwen2.5-7b-instruct"
QWEN_MODEL_DEVOPS="qwen2.5-14b-instruct"

# ============================================
# VERCEL CONFIGURATION
# ============================================
# Vercel Sandbox (Required if SANDBOX_PROVIDER=vercel)
SANDBOX_VERCEL_TOKEN=your_vercel_token_here
SANDBOX_VERCEL_TEAM_ID=your_team_id_here
SANDBOX_VERCEL_PROJECT_ID=your_project_id_here

# Vercel AI Gateway (Optional)
AI_GATEWAY_API_KEY=your_ai_gateway_key_here

# ============================================
# GOOGLE SERVICES (Optional)
# ============================================
GOOGLE_SEARCH_API_KEY=your_google_search_key_here

# ============================================
# FACEBOOK/META COMMERCE (Optional)
# ============================================
FACEBOOK_TOKEN=your_facebook_token_here
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
FACEBOOK_CATALOG_ID=your_facebook_catalog_id_here

# ============================================
# HUGGING FACE (Optional)
# ============================================
HF_TOKEN=your_huggingface_token_here
HUGGINGFACE_API_KEY=your_huggingface_key_here

# ============================================
# PAYMENT GATEWAY - ASAAS (Optional)
# ============================================
ASAAS_API_KEY=your_asaas_api_key_here

# ============================================
# SOLAR & ENERGY SERVICES (Optional)
# ============================================
# NREL SAM (Solar Analysis Module)
NREL_SAM_API_KEY=your_nrel_sam_api_key_here

# EDELTEC (Energy Distribution)
EDELTEC_ACCESS_KEY=your_edeltec_access_key_here
EDELTEC_PASSWORD=your_edeltec_password_here

# ============================================
# DEVELOPMENT TOOLS (Optional)
# ============================================
# GitLens
GITLENS_LICENSE_KEY=your_gitlens_license_key_here

# ============================================
# CLOUD SERVICES (Optional)
# ============================================
# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Microsoft Clarity
MICROSOFT_CLARITY_TOKEN=your_microsoft_clarity_token_here

# Context7 MCP
CONTEXT7_API_KEY=your_context7_api_key_here
CONTEXT7_MCP_URL=mcp.context7.com/mcp
CONTEXT7_API_URL=context7.com/api/v1

# Pydantic Logfire
PYDANTIC_LOGFIRE_TOKEN=your_pydantic_logfire_token_here

# ============================================
# COMMUNICATION SERVICES (Optional)
# ============================================
# Telegram API
TELEGRAM_API_ID=your_telegram_api_id_here
TELEGRAM_API_HASH=your_telegram_api_hash_here

# ============================================
# MAPPING & GIS SERVICES (Optional)
# ============================================
# Cesium Ion (3D Mapping)
CESIUM_ION_TOKEN=your_cesium_ion_token_here
